# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Supabase Service Role (for Edge Functions only - DO NOT expose to client)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# OpenAI API (for proposal generation and voice parsing)
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API (alternative to OpenAI)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Environment
NODE_ENV=development

# Optional: Wealth Screening API
WEALTH_SCREENING_API_KEY=your-wealth-api-key-here
WEALTH_SCREENING_API_URL=https://api.wealth-screening.example.com
